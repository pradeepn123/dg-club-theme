<a
  href="{{ url }}"
  title="{{ title | escape }}"
  role="link"
  class="text-button {{ class }} text-button--has-icon"
  {% if disabled %}
    aria-disabled="true"
  {% endif %}
  {% if block %}
    {{- block.shopify_attributes -}}
  {% endif %}
>
  {{- title }}
  <span class="right_arrow_icon">{% render 'svg-icons' with 'right-arrow' %}</span>
</a>
